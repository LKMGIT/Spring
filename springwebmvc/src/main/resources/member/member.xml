<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ssg.springwebmvc.membermvc.mapper.MemberMapper">

    <!-- INSERT: 파라미터는 Member -->
    <insert id="insert" parameterType="com.ssg.springwebmvc.membermvc.dto.MemberDTO"
            useGeneratedKeys="true" keyProperty="id">
        INSERT INTO tmember (mid, mpw, mname, mDate)
        VALUES (#{mid}, #{mpw}, #{mname}, #{mDate})
    </insert>

    <!-- SELECT: 전체 조회 -->
    <select id="findAll" resultType="com.ssg.springwebmvc.membermvc.dto.MemberDTO">
        SELECT
        *
        FROM tmember
    </select>

</mapper>